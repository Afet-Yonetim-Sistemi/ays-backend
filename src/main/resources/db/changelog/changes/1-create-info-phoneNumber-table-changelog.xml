<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.10.xsd">

    <changeSet id="1" author="noyan">
        <createTable tableName="phone_numbers">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="create_date" type="timestamp" defaultValueDate="CURRENT_TIMESTAMP" />
            <column name="update_date" type="timestamp" defaultValueDate="CURRENT_TIMESTAMP" />
            <column name="country_code" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="line_number" type="int">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>